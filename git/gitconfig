[filter "lfs"]
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
  clean = git-lfs clean -- %f
[user]
  name = Jeremy Marquis
  email = jeremy@jeremymarquis.com
[alias]
  co = checkout
  p = pull --rebase --stat
  s = status
  b = branch
  bs = branch --sort=-committerdate --column
  c = commit
  d = diff
  a = add
  rpull = pull --rebase --stat
[core]
  excludesfile = ~/.gitignore
  preloadindex = true
[push]
  default = current
